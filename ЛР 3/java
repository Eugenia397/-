1) БИНАРНАЯ КУЧА
import java.util.PriorityQueue;

public class BinaryHeapDemo {
    public static void main(String[] args) {
        PriorityQueue<Integer> minHeap = new PriorityQueue<>(); // Min-heap by default
        int[] numbers = {10, 5, 8, 15};
        
        for (int num : numbers) {
            minHeap.offer(num); // Adding items to the heap
        }
        
        // Extracting and printing minimal elements one-by-one
        while (!minHeap.isEmpty()) {
            System.out.print(minHeap.poll() + " ");
        }
    }
}



2) БИНОМИНАЛЬНАЯ КУЧА
import java.util.ArrayList;
import java.util.List;

public class BinomialTree {
    public int key;
    public List<BinomialTree> children;

    public BinomialTree(int k) {
        key = k;
        children = new ArrayList<>();
    }
}

public class BinomialHeap {
    private List<BinomialTree> trees;

    public BinomialHeap() {
        trees = new ArrayList<>();
    }

    public void insert(int key) {
        BinomialTree newTree = new BinomialTree(key);
        BinomialHeap tempHeap = new BinomialHeap();
        tempHeap.trees.add(newTree);
        merge(tempHeap);
    }

    private void merge(BinomialHeap other) {
        List<BinomialTree> mergedTrees = new ArrayList<>(trees);
        mergedTrees.addAll(other.trees);
        // Необходима логика для реорганизации деревьев...
    }
}



3) КУЧА ФИБОНАЧЧИ
    public class FibonacciHeapNode {
    public int key;
    public FibonacciHeapNode parent, child, left, right;
    public boolean marked;
    public int degree;

    public FibonacciHeapNode(int key) {
        this.key = key;
    }
}

public class FibonacciHeap {
    private FibonacciHeapNode minNode;
    private int size;

    public FibonacciHeap() {
        this.minNode = null;
        this.size = 0;
    }

    public void insert(int key) {
        FibonacciHeapNode node = new FibonacciHeapNode(key);
        if (minNode == null) {
            minNode = node;
        } else {
            addToRootList(node);
            if (node.key < minNode.key) {
                minNode = node;
            }
        }
        size++;
    }

    private void addToRootList(FibonacciHeapNode node) {
        if (minNode == null) {
            node.left = node.right = node;
            minNode = node;
        } else {
            node.right = minNode.right;
            node.left = minNode;
            minNode.right.left = node;
            minNode.right = node;
            if (node.key < minNode.key) {
                minNode = node;
            }
        }
    }

    public FibonacciHeapNode extractMin() {
        // Логика экстракции минимального элемента
        return null;
    }

    public void decreaseKey(FibonacciHeapNode node, int newValue) {
        // Логика уменьшения ключа
    }

    public void delete(FibonacciHeapNode node) {
        // Логика удаления узла
    }
}




4) ХЭШ-ТАБЛИЦА
import java.util.HashMap;
import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        HashMap<String, Integer> hashTable = new HashMap<>();
        hashTable.put("apple", 1);
        hashTable.put("banana", 2);
        hashTable.put("orange", 3);

        System.out.print("Введите название фрукта: ");
        String fruit = scanner.nextLine();
        if (hashTable.containsKey(fruit)) {
            System.out.println("Количество " + fruit + ": " + hashTable.get(fruit));
        } else {
            System.out.println("Такой фрукт не найден.");
        }
    }
}

